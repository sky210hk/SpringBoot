	<insert id="regist">

insert into PRODUCT(prod_enddate
								,prod_category
								,prod_writer
								,prod_name
								,prod_price
								,prod_count
								,prod_discount
								,prod_purchase_yn
								,prod_content
								,prod_comment) 

		  values(#{prod_enddate}
				,#{prod_category}
				,#{prod_writer}
				,#{prod_name}
				,#{prod_price}
				,#{prod_count}
				,#{prod_discount}
				,#{prod_purchase_yn}
				,#{prod_content}
				,#{prod_comment})
				
</insert>
</mapper>